{
    "name": "voting-app-client",
    "version": "1.1.0",
    "description": "Client for Voting App - A Free Code Camp Dymamic Web Application Project.",
    "private": true,
    "scripts": {
        "build-css": "node-sass-chokidar src/sass/ -o src/css/",
        "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/css/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "react-scripts start",
        "dev": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
    },
    "keywords": [
        "voting-app",
        "react",
        "client-side"
    ],
    "author": "minhkashyu",
    "license": "MIT",
    "dependencies": {
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-scripts": "1.0.10",
        "axios": "^0.16.2",
        "node-sass-chokidar": "^0.0.3",
        "npm-run-all": "^4.0.2",
        "prop-types": "^15.5.10",
        "react-cookie": "^2.0.8",
        "react-redux": "^5.0.5",
        "react-router-dom": "^4.1.2",
        "redux": "^3.7.2",
        "redux-form": "^7.0.1",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
    }
}
